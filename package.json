{
  "private": true,
  "packageManager": "pnpm@7.1.5",
  "engines": {
    "node": ">=16.7"
  },
  "main": "./dist/worker.mjs",
  "scripts": {
    "build": "pnpm build:css && pnpm build:remix",
    "build:css": "tailwindcss -m -i styles/app.css -o app/styles/app.css",
    "build:remix": "NODE_ENV=production remix build",
    "clean": "rm -rf .cache app/styles/app.css build public/build",
    "dev": "pnpm build && run-p dev:*",
    "dev:css": "tailwindcss -w -i styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "NODE_ENV=development node support/run-dev.cjs 'wrangler pages dev ./public --kv SAVEDFORLATER_DATA -b FEEDBIN_API_KEY=$FEEDBIN_API_KEY -b TWITTER_API_KEY=$TWITTER_API_KEY'",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "start": "NODE_ENV=production pnpm dev:wrangler",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@remix-run/cloudflare": "^1.6.0",
    "@remix-run/cloudflare-pages": "^1.6.0",
    "@remix-run/react": "^1.6.0",
    "dataloader": "^2.1.0",
    "date-fns": "^2.28.0",
    "emoji-regex": "^10.1.0",
    "feed": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@cloudflare/workers-types": "^3.13.0",
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@types/node": "^16",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-pkg": "^0.15.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.14"
  }
}
